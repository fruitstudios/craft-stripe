{
    "name": "fruitstudios/stripe",
    "description": "Developer tools for Stripe inc Charges & Connected Accounts.",
    "type": "craft-plugin",
    "version": "1.0.4",
    "keywords": [
        "craft",
        "cms",
        "craftcms",
        "stripe"
    ],
    "support": {
        "docs": "https://github.com/fruitstudios/craft-stripe/blob/master/README.md",
        "issues": "https://github.com/fruitstudios/craft-stripe/issues"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Fruit Studios",
            "homepage": "https://fruitstudios.co.uk"
        }
    ],
    "require": {
        "craftcms/cms": "^3.0.0-RC1",
        "stripe/stripe-php": "^6.7"
    },
    "autoload": {
        "psr-4": {
          "fruitstudios\\stripe\\": "src/"
        }
    },
    "extra": {
        "name": "Stripe",
        "handle": "stripe",
        "hasCpSettings": true,
        "hasCpSection": false,
        "changelogUrl": "https://raw.githubusercontent.com/fruitstudios/craft-stripe/master/CHANGELOG.md",
        "components": {
            "listService": "fruitstudios\\stripe\\services\\ListService",
            "subscriptionService": "fruitstudios\\stripe\\services\\SubscriptionService"
        },
        "class": "fruitstudios\\stripe\\Stripe"
    }
}
